{
	"name": "boilerplate",
	"productName": "ING Web ToolKit",
	"version": "0.0.0",
	"license": "MIT",
	"angular-cli": {},
	"scripts": {
		"build-electron": "ng build --base-href . && cp -rf src/electron/* dist ; rm -rf dist/assets/* ; gulp deploy",
		"start": "rm -rf dist ;  npm run build-electron ; cp -rf src/app dist/app ; electron dist",
		"deploy": "rm -rf dist ;  npm run build-electron ; cp -rf src/app dist/app",
		"package-mac": "npm run deploy ; electron-packager dist --overwrite --platform=darwin --arch=x64  --prune=true --out=releases",
		"package-win": "electron-packager . boilerplate --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=DigitalSC --version-string.FileDescription=DigitalSC --version-string.ProductName=\"My Product Name\"",
		"test": "ng test",
		"lint": "ng lint",
		"e2e": "ng e2e"
	},
	"private": true,
	"dependencies": {
		"@angular/common": "^2.4.0",
		"@angular/compiler": "^2.4.0",
		"@angular/core": "^2.4.0",
		"@angular/forms": "^2.4.0",
		"@angular/http": "^2.4.0",
		"@angular/platform-browser": "^2.4.0",
		"@angular/platform-browser-dynamic": "^2.4.0",
		"@angular/router": "^3.4.0",
		"core-js": "^2.4.1",
		"rxjs": "^5.1.0",
		"zone.js": "^0.7.6"
	},
	"devDependencies": {
		"@angular/cli": "1.0.0-beta.32.3",
		"@angular/compiler-cli": "^2.4.0",
		"@types/jasmine": "2.5.38",
		"@types/node": "~6.0.60",
		"codelyzer": "~2.0.0-beta.4",
		"electron": "^1.4.1",
		"gulp": "^3.9.1",
		"gulp-autoprefixer": "^3.1.1",
		"gulp-concat": "^2.6.1",
		"gulp-imagemin": "^3.1.1",
		"gulp-minify-css": "^1.2.4",
		"gulp-plumber": "^1.1.0",
		"gulp-sass": "^3.1.0",
		"gulp-sequence": "^0.4.6",
		"gulp-shell": "^0.5.2",
		"gulp-sourcemaps": "^2.4.1",
		"gulp-uglify": "^2.0.1",
		"gulp-util": "^3.0.8",
		"jasmine-core": "~2.5.2",
		"jasmine-spec-reporter": "~3.2.0",
		"karma": "~1.4.1",
		"karma-chrome-launcher": "~2.0.0",
		"karma-cli": "~1.0.1",
		"karma-coverage-istanbul-reporter": "^0.2.0",
		"karma-jasmine": "~1.1.0",
		"karma-jasmine-html-reporter": "^0.2.2",
		"protractor": "~5.1.0",
		"ts-node": "~2.0.0",
		"tslint": "~4.4.2",
		"typescript": "~2.0.0"
	}
}
